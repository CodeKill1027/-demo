<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.Springcloud.dao.usermapper" >


    <select id="queryRoleall" resultType="com.example.Springcloud.pojo.Role">
        select * from  Role
    </select>
    <select id="queryName" resultType="com.example.Springcloud.pojo.Role">
        select username,permissions1,permissions2,permissions3  from Role where username=#{username}

    </select>


</mapper>

