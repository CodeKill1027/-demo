<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.security.dao.usermapper" >


   <select id="accordingToQuerySYUser" resultType="com.example.security.entity.SysUser" parameterType="String">
      select  * from  SysUser where username =#{username}
   </select>

   <select id="accordingToQueryUser" resultType="com.example.security.entity.user" parameterType="String">
      select  * from  SysUser where username =#{username}
   </select>



</mapper>

